  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.6.2",
    "@types/node": "^20.14.15",
    "@typescript-eslint/parser": "^8.0.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.0",
    "terser": "^5.31.5",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@literate.ink/utilities": "1.0.0-11713907881.1",
    "js-base64": "^3.7.7"
